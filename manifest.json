{
    "manifest_version": 2,
    "name": "word-detecter",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "word detect"
    } 
}